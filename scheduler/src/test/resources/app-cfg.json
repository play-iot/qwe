{
  "__app__": {
    "__scheduler__": {
      "schedulerName": "qwe",
      "registerServiceClass": "io.zero88.qwe.scheduler.service.SchedulerRegisterService",
      "registerAddress": "qwe.scheduler.register",
      "monitorAddress": "qwe.scheduler.monitor",
      "__schedule_worker__": {
        "poolName": "worker-pool-scheduler-qwe",
        "poolSize": 5,
        "maxExecuteTime": 60,
        "maxExecuteTimeUnit": "SECONDS"
      }
    }
  }
}
